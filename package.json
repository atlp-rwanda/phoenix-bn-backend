{
  "name": "atlp-project_phase",
  "version": "1.0.0",
  "description": "Make company global travel and accommodation easy and convenient for the strong workforce of savvy members of staff, by leveraging the modern web.",
  "main": "index.js",
  "scripts": {
    "start": "npm run lint && npm run db:migrate && npm run db:seed:undo:all && npm run db:seed &&  nodemon --exec babel-node ./src/index.js",
    "lint": "./node_modules/.bin/eslint src/ --fix",
    "db:migrate": "./node_modules/.bin/sequelize db:migrate",
    "db:migrate:undo:all": "./node_modules/.bin/sequelize db:migrate:undo:all",
    "db:migrate:undo": "./node_modules/.bin/sequelize db:migrate:undo",
    "db:seed": "./node_modules/.bin/sequelize db:seed:all",
    "db:seed:undo:all": "./node_modules/.bin/sequelize db:seed:undo:all",
    "test": "mocha --timeout 50000 --require @babel/polyfill --require @babel/register src/tests/index.js  --exit || true",
    "coverage": "nyc --reporter=lcov --reporter=text npm run test",
    "dev": "nodemon --exec babel-node ./src/index.js"
  },
  "author": "Team Phoniex",
  "license": "ISC",
  "dependencies": {
    "@babel/cli": "^7.12.0",
    "@babel/plugin-transform-runtime": "^7.12.0",
    "@babel/polyfill": "^7.11.5",
    "@babel/register": "^7.12.0",
    "@babel/runtime": "^7.12.0",
    "bcrypt": "^5.0.0",
    "chai": "^4.2.0",
    "chai-http": "^4.3.0",
    "crypto": "^1.0.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "joi": "^17.2.1",
    "jsonwebtoken": "^8.5.1",
    "mailgun-js": "^0.22.0",
    "mocha": "^8.1.3",
    "nodemailer": "^6.4.14",
    "nodemailer-mailgun-transport": "^2.0.1",
    "pg": "^8.4.1",
    "pg-hstore": "^2.3.3",
    "sequelize": "^6.3.5",
    "sequelize-cli": "^6.2.0",
    "sinon": "^9.2.0",
    "swagger-jsdoc": "^4.2.3",
    "swagger-ui-express": "^4.1.4"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/node": "^7.10.5",
    "@babel/preset-env": "^7.11.5",
    "eslint": "^7.11.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.22.1",
    "nodemon": "^2.0.4",
    "nyc": "^15.1.0"
  }
}
