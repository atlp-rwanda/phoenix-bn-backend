{
  "name": "atlp-project_phase",
  "version": "1.0.0",
  "description": "Make company global travel and accommodation easy and convenient for the strong workforce of savvy members of staff, by leveraging the modern web.",
  "main": "index.js",
  "scripts": {
    "start": "npm run db:migrate && npm run db:seed &&  nodemon --exec babel-node ./src/index.js",
    "lint": "./node_modules/.bin/eslint src/ --fix",
    "db:migrate": "./node_modules/.bin/sequelize db:migrate",
    "db:migrate:undo:all": "./node_modules/.bin/sequelize db:migrate:undo:all",
    "db:migrate:undo": "./node_modules/.bin/sequelize db:migrate:undo",
    "db:seed": "./node_modules/.bin/sequelize db:seed:all",
    "db:seed:undo:all": "./node_modules/.bin/sequelize db:seed:undo:all",
    "test": "npm run db:migrate:undo:all && npm run db:migrate && npm run db:seed && mocha --timeout 50000 --require @babel/polyfill --require @babel/register src/tests/index.js  --exit || true",
    "coverage": "nyc --reporter=lcov --reporter=text npm run test",
    "dev": "npm run lint  && nodemon --exec babel-node ./src/index.js"
  },
  "author": "Team Phoniex",
